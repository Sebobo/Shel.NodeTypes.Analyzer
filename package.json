    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "lodash.omit": "^4.3.0",
    "lodash.throttle": "^4.0.8",
    "plow-js": "^2.2.0",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-close-on-escape": "^3.0.0",
    "react-collapse": "^5.0.1",
    "react-dnd": "7.0.2",
    "react-dnd-html5-backend": "~7.0.2",
