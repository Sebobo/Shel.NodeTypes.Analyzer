    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "destiny": "^0.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "postcss-custom-properties": "^9.1.1",
    "postcss-modules": "^1.5.0",
    "prettier": "^1.19.1",
